{
  "name": "prisma-safety",
  "version": "0.0.4",
  "description": "A safe migration checker for Prisma.",
  "repository": {
    "type": "git",
    "url": "https://github.com/loop-payments/prisma-safety.git"
  },
  "license": "MIT",
  "author": "engineering@loop.com",
  "type": "module",
  "imports": {
    "#src/*": "./dist/*"
  },
  "bin": "dist/cli.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
    "build": "tsc",
    "bump-version": "node ./scripts/bump-version.js",
    "setup:precommit-hooks": "husky install",
    "style:eslint": "eslint src --fix --max-warnings=0 --cache",
    "style:eslint:check": "eslint src --max-warnings=0 --cache",
    "style:prettier": "prettier --write src",
    "style:prettier:check": "prettier --check src",
    "test": "node ./node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "@mrleebo/prisma-ast": "^0.10.0",
    "commander": "^11.0.0",
    "read-package-up": "^11.0.0"
  },
  "devDependencies": {
    "@tsconfig/esm": "^1.0.3",
    "@tsconfig/node18": "^18.2.0",
    "@tsconfig/strictest": "^2.0.1",
    "@types/jest": "^29.2.5",
    "@types/node": "^20.2.5",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-canonical": "^4.9.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jest": "^27.2.1",
    "husky": "^9.0.6",
    "jest": "^29.3.1",
    "lint-staged": "^15.0.1",
    "prettier": "3.2.5",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.3.1"
}
